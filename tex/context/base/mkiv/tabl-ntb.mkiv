   \edef\m_tabl_tbl_level{\the\c_tabl_level}}

\unexpanded\def\tabl_ntb_prev_level
  {\advance\c_tabl_level\minusone
   \edef\m_tabl_tbl_level{\the\c_tabl_level}}

