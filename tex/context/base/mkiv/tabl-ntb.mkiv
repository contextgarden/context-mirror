\let\m_tabl_ntb_before_split\empty
\let\m_tabl_ntb_after_split \empty
\let\m_tabl_ntb_same_page   \empty

% split + page:
%
% \bTABLE[split=yes]
% \bTR \bTD left \eTD\bTD right \eTD\eTR
% \bTR[after=\page] \bTD left \eTD\bTD right \eTD\eTR
% \bTR \bTD left \eTD\bTD right \eTD\eTR
