    <cd:command name="framed" file="pack-rul.mkiv">
        <cd:arguments>
            <cd:assignments list="yes" optional="yes">
                <cd:inherit name="setupframed"/>
            </cd:assignments>
            <cd:content/>
        </cd:arguments>
    </cd:command>

    <cd:command name="framed" generated="yes" variant="generated" file="pack-rul.mkiv">
        <cd:sequence>
