                <cd:resolve name="argument-name"/>
            </cd:arguments>
        </cd:command>

        <cd:command name="doactivateinjector" level="system" file="typo-inj.mkiv">
            <cd:arguments>
                <cd:resolve name="argument-name"/>
            </cd:arguments>
        </cd:command>

        <cd:command name="dotestinjector" level="system" file="typo-inj.mkiv">
            <cd:arguments>
                <cd:resolve name="argument-name"/>
            </cd:arguments>
        </cd:command>

    -->

</cd:interface>