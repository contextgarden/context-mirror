% helpers

\protected\def\mmlexecutecommand#1%
  {\ifcsname#1\endcsname
     \expandafter\firstoftwoarguments
   \else
     \expandafter\secondoftwoarguments
   \fi
   \lastnamedcs}

\let\mmlextensible\mathml_extensible

